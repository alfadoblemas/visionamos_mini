<%= form_for(@usuario) do |f| %>
  <% if @usuario.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@usuario.errors.count, "error") %> prohibited this usuario from being saved:</h2>

      <ul>
      <% @usuario.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :nombre %><br>
    <%= f.text_field :nombre %>
  </div>
  <div class="field">
    <%= f.label :apellido %><br>
    <%= f.text_field :apellido %>
  </div>
  <div class="field">
    <%= f.label :tipo_identificacion %><br>
    <%= f.select :tipo_identificacion, options_for_select(@usuario_decorated.tipo_identificacion_options) %>
  </div>
  <div class="field">
    <%= f.label :nro_identificacion %><br>
    <%= f.text_field :nro_identificacion %>
  </div>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :tipo_usuario %><br>
    <%= f.number_field :tipo_usuario %>
  </div>
  <div class="field">
    <%= f.label :agencia_id %><br>
    <%= f.text_field :agencia_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
